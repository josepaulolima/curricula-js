{
  "name": "curricula-js",
  "version": "1.0.0",
  "description": "Malla curricular Full Stack JavaScript @Laboratoria",
  "scripts": {
    "mdlint": "mdlint .",
    "eslint": "eslint topics/javascript",
    "validate": "cd topics && course-parser browser/README.md css/README.md html/README.md javascript/README.md scm/README.md shell/README.md --track=js --locale=es-ES --validate",
